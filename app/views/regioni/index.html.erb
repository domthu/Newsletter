<h1>Listing regioni</h1>

<table>
  <tr>
    <th>Idregione</th>
    <th>Regione</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @regioni.each do |regione| %>
  <tr>
    <td><%= regione.idregione %></td>
    <td><%= regione.regione %></td>
    <td><%= link_to 'Show', regione %></td>
    <td><%= link_to 'Edit', edit_regione_path(regione) %></td>
    <td><%= link_to 'Destroy', regione, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Regione', new_regione_path %>
